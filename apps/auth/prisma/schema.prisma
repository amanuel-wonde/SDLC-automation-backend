generator client {
    provider = "prisma-client-js"
    output   = "../../../node_modules/@prisma/client-auth"
}

datasource db {
    provider = "postgresql"
}

model User {
    id        String   @id @default(uuid())
    name      String
    email     String   @unique
    password  String
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt

    @@map("auth_users")
}
